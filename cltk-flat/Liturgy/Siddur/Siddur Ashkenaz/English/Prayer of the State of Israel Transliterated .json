{
    "text": {
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 0_Paragraph": "Avinu she-bashamayim,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 1_Paragraph": "tzur yisra'el vego'alo,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 2_Paragraph": "barekh et medinat yisra'el,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 3_Paragraph": "reshit tzemichat ge'ulatenu.",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 4_Paragraph": "hagen aleiha be'evrat chasdecha,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 5_Paragraph": "u-feros aleiha sukkat shelomekha,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 6_Paragraph": "u-shelach orecha va'amitecha le-rasheiha, sareiha veyo'atzeiha,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 7_Paragraph": "vetakkenem be'etzah tovah milefanekha.",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 8_Paragraph": "chazzek et yedei meginnei eretz kodshenu,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 9_Paragraph": "vehanchilem eloheinu yeshu'ah",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 10_Paragraph": "va'ateret nitzachon te'atterem,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 11_Paragraph": "venatata shalom ba'aretz,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 12_Paragraph": "vesimchat olam leyosheveiha.",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 13_Paragraph": "ve'et acheinu kol beit yisra'el,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 14_Paragraph": "pekod na bechol artzot pezureihem,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 15_Paragraph": "vetolichem meherah komemiyut letziyon irecha",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 16_Paragraph": "velirushalayim mishkan shemecha,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 17_Paragraph": "ka-katuv betorat mosheh avdech:",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 18_Paragraph": "\"'im yihyeh niddachacha biktzeh hashamayim,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 19_Paragraph": "misham yekabbetzcha hashem eloheicha umisham yikkachecha.",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 20_Paragraph": "vehevi'acha hashem eloheicha el ha'aretz asher yareshu avoteicha virishtah,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 21_Paragraph": "veheitivcha vehirbecha me'avoteicha.",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 22_Paragraph": "umal hashem eloheicha et levavecha ve'et levav zar'echa,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 23_Paragraph": "le'ahavah et hashem eloheicha bechol levavecha uvechol nafshecha,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 24_Paragraph": "lema'an chayeicha\".",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 25_Paragraph": "veyached levavenu le'ahavah uleyir'ah et shemecha,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 26_Paragraph": "velishmor et chol divrei toratecha,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 27_Paragraph": "ushelach lanu meherah ben david meshiach tzidkecha,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 28_Paragraph": "lifdot mechakkei ketz yeshu'atecha.",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 29_Paragraph": "hofa bahadar ge'on uzzecha",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 30_Paragraph": "'al chol yoshevei tevel artzecha,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 31_Paragraph": "veyomar kol asher neshamah be'appo.",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 32_Paragraph": "\"hashem elohei yisra'el melech,",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 33_Paragraph": "umalchuto bakkol mashalah!\"",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 1_Prayer of the State of Israel, 34_Paragraph": "amen selah.",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 2_Prayer for Israeli Soldiers, 0_Paragraph": "Mi shebberach avoteinu avraham yitzchak veya'akov",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 2_Prayer for Israeli Soldiers, 1_Paragraph": "hu yevarech et chayalei tzeva hagannah leyisra'el",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 2_Prayer for Israeli Soldiers, 2_Paragraph": "ha'omedim al mishmar artzenu ve'arei eloheinu",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 2_Prayer for Israeli Soldiers, 3_Paragraph": "miggevul ha-levanon ve'ad midbar mitzrayim",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 2_Prayer for Israeli Soldiers, 4_Paragraph": "u'min hayam haggadol ad levo ha'aravah bayabbashah ba'avir uvayam",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 2_Prayer for Israeli Soldiers, 5_Paragraph": "yitten hashem et oyeveinu hakkamim aleinu niggafim lifneihem",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 2_Prayer for Israeli Soldiers, 6_Paragraph": "hakkadosh baruch hu yishmor veyatzil et chayaleinu mikkol tzarah vetzukah umikkal nega umachlah",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 2_Prayer for Israeli Soldiers, 7_Paragraph": "veyishlach berachah vehatzlachah bechol ma'aseh yedeihem",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 2_Prayer for Israeli Soldiers, 8_Paragraph": "yadber sone'einu tachteihem vi'atrem becheter yeshu'ah uve'ateret nitzachon",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 2_Prayer for Israeli Soldiers, 9_Paragraph": "vikyam bahem hakkatuv.",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 2_Prayer for Israeli Soldiers, 10_Paragraph": "ki hashem eloheichem haholech immachem lehillachem lachem im oyeveichem lehoshia etchem",
        "1_Shabbat, 3_Shacharit, 5_Communal Prayers, 2_Prayer for Israeli Soldiers, 11_Paragraph": "venomar amen\n"
